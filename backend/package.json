{
  "name": "zeltlager-backend",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "bin": {
    "tent-camp": "bin/tent-camp.js"
  },
  "scripts": {
    "deploy": "npm run build && cdk deploy --all",
    "build": "tsc ./lambda/imageTransform/index.ts && tsc ./lambda/objectCreated/index.ts",
    "ses-update-template": "aws ses update-template --cli-input-json --region eu-central-1 file://successful-registration-template.json && aws ses update-template --cli-input-json --region eu-central-1 file://waiting-list-registration-template.json",
    "synth": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/google-spreadsheet": "^3.3.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mailparser": "^2.7.4",
    "@types/node": "14.14.6",
    "@types/nodemailer": "^6.4.0",
    "@types/sharp": "^0.26.0",
    "@types/source-map-support": "^0.5.3",
    "aws-cdk": "^1.183.0",
    "aws-sdk": "^2.787.0",
    "esbuild": "^0.14.11",
    "typescript": "~4.0.5"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.183.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.183.0",
    "@aws-cdk/aws-dynamodb": "1.183.0",
    "@aws-cdk/aws-lambda": "1.183.0",
    "@aws-cdk/aws-lambda-event-sources": "1.183.0",
    "@aws-cdk/aws-lambda-nodejs": "1.183.0",
    "@aws-cdk/aws-s3": "1.183.0",
    "@aws-cdk/aws-ses": "^1.183.0",
    "@aws-cdk/aws-ses-actions": "^1.183.0",
    "@aws-cdk/core": "1.183.0",
    "@azure/cognitiveservices-computervision": "^7.1.0",
    "@azure/ms-rest-azure-js": "^2.0.1",
    "dotenv": "^8.2.0",
    "google-spreadsheet": "^3.3.0",
    "jsonwebtoken": "^8.5.1",
    "mailparser": "^3.0.0",
    "nanoid": "^4.0.0",
    "nodemailer": "^6.4.15",
    "source-map-support": "^0.5.19",
    "zod": "^1.11.10"
  },
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}
