{
  "name": "zeltlager-backend",
  "version": "0.1.0",
  "bin": {
    "tent-camp": "bin/tent-camp.js"
  },
  "scripts": {
    "deploy": "npm run build && cdk deploy",
    "build": "tsc ./lambda/imageTransform/index.ts && tsc ./lambda/objectCreated/index.ts",
    "synth": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.61",
    "@types/google-spreadsheet": "^3.0.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mailparser": "^2.7.3",
    "@types/node": "14.10.1",
    "@types/nodemailer": "^6.4.0",
    "@types/sharp": "^0.26.0",
    "@types/source-map-support": "^0.5.3",
    "aws-cdk": "1.62.0",
    "aws-sdk": "^2.751.0",
    "typescript": "~4.0.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.61.1",
    "@aws-cdk/aws-dynamodb": "1.61.1",
    "@aws-cdk/aws-lambda": "1.61.1",
    "@aws-cdk/aws-lambda-event-sources": "1.61.1",
    "@aws-cdk/aws-lambda-nodejs": "1.61.1",
    "@aws-cdk/aws-s3": "1.61.1",
    "@aws-cdk/aws-ses": "^1.61.1",
    "@aws-cdk/aws-ses-actions": "^1.61.1",
    "@aws-cdk/core": "1.61.1",
    "@azure/cognitiveservices-computervision": "^7.0.1",
    "@azure/ms-rest-azure-js": "^2.0.1",
    "dotenv": "^8.2.0",
    "google-spreadsheet": "^3.0.13",
    "jsonwebtoken": "^8.5.1",
    "mailparser": "^3.0.0",
    "nanoid": "^3.1.12",
    "nodemailer": "^6.4.11",
    "source-map-support": "^0.5.19",
    "zod": "^1.11.6"
  },
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}