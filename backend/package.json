{
  "name": "zeltlager-backend",
  "version": "0.1.0",
  "bin": {
    "tent-camp": "bin/tent-camp.js"
  },
  "scripts": {
    "deploy": "npm run build && cdk deploy",
    "build": "tsc ./lambda/imageTransform/index.ts && tsc ./lambda/objectCreated/index.ts",
    "synth": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/node": "14.0.27",
    "@types/sharp": "^0.25.1",
    "@types/source-map-support": "^0.5.2",
    "aws-cdk": "1.56.0",
    "aws-sdk": "^2.734.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.56.0",
    "@aws-cdk/aws-dynamodb": "1.56.0",
    "@aws-cdk/aws-lambda": "1.56.0",
    "@aws-cdk/aws-lambda-event-sources": "1.56.0",
    "@aws-cdk/aws-lambda-nodejs": "1.56.0",
    "@aws-cdk/aws-s3": "1.56.0",
    "@aws-cdk/core": "1.56.0",
    "@azure/cognitiveservices-computervision": "^7.0.1",
    "@azure/ms-rest-azure-js": "^2.0.1",
    "@types/aws-lambda": "^8.10.61",
    "dotenv": "^8.2.0",
    "nanoid": "^3.1.12",
    "source-map-support": "^0.5.19",
    "zod": "^1.10.2"
  },
  "cdk-lambda": "/asset-output/index.js",
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}